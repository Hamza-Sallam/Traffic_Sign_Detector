{
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/frontend/$1"
        },
        {
            "src": "/(.*)",
            "dest": "backend/main.py"
        }
    ]
}